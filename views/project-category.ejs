<html>
<head>
	<%- include parts/prehead.ejs %>
</head>
<body>
	<%- include parts/pageheading.ejs %>

	<h1>
		my projects for category: <%= category.name %>
	</h1>
	
	<% for (var key in category.projects) { %>
		<% var project = category.projects[key] %>
		
		<div><a href="/project/<%= project.key %>">
			<%= project.name %>
		</a></div>
	<% } %>
</body>
</html>