extends ../layouts/layout.jade
include ../mixins/navbar.jade

block body
	+navbar("navbar", {
			initFixed: true
		})
	
	h1 My projects for category: #{category.name}
	
	- for (var key in category.projects) {
		- var project = category.projects[key];
		
		div: a(href=URLS.projectDetail(project.key))
			| #{project.name}
	- }